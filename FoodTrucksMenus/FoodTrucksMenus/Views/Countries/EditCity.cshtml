
@model FoodTrucksMenus.Models.CityViewModel

@{
    ViewData["Title"] = "Create";
}
<div class="container mt-5">
    <h1>Crear</h1>

    <h4>Ciudad</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="EditCity">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="StateId" />
                <partial name="_City" />
                <div class="form-group mt-2">
                    <input type="submit" value="Crear" class="btn btn-outline-primary" />
                    <a asp-action="DetailsStates" asp-route-id="@Model?.StateId" class="btn btn-outline-info">Regresar</a>
                </div>
            </form>
        </div>
    </div>


</div>

@section Scripts {
    @{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");
}
} 